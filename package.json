{
  "name": "amq-node-js",
  "version": "2.3.6",
  "description": "Local client for Anime Music Quiz that supports Discord rich presence",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "keywords": [
    "amq",
    "anime",
    "music",
    "quiz",
    "discord",
    "rich",
    "presence",
    "rpc"
  ],
  "author": "LittleEndu",
  "license": "MIT",
  "devDependencies": {
    "electron": "^11.1.0",
    "electron-builder": "^22.9.1"
  },
  "dependencies": {
    "discord-rpc": "^3.1.4",
    "electron-window-state": "^5.0.3",
    "get-folder-size": "^2.0.1",
    "github-version-checker": "^2.2.0",
    "open": "^7.3.0",
    "uuid": "^8.3.2"
  },
  "build": {
    "appId": "com.electron.amq.client",
    "productName": "AMQ node.js",
    "copyright": "Copyright Â© 2020 LittleEndu",
    "win": {
      "target": [
        "nsis",
        "portable"
      ],
      "icon": "favicon.ico"
    }
  }
}
